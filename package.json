{
  "name": "typescript-starter-kit",
  "version": "1.0.0",
  "description": "typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "webpack --mode production ",
    "dev": "webpack --mode development",
    "precommit": "lint-staged",
    "lint": "eslint \"./src/js/**/*.ts\" --quiet --fix; exit 0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Takeshi-005/typescript-stater-kit.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Takeshi Odaka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Takeshi-005/typescript-stater-kit/issues"
  },
  "homepage": "https://github.com/Takeshi-005/typescript-stater-kit#readme",
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/node": "11.13.10",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "autoprefixer": "9.4.10",
    "axios": "^0.18",
    "babel-loader": "^8.0.5",
    "css-loader": "2.1.0",
    "es6-promise": "4.2.6",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "1.3.7",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "3.0.1",
    "husky": "1.3.1",
    "jest": "24.8.0",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.17.0",
    "prettier-stylelint": "0.4.2",
    "pubsub-js": "1.7.0",
    "pug": "2.0.0",
    "pug-loader": "^2.4.0",
    "sass-loader": "7.1.0",
    "stats-webpack-plugin": "0.7.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.1.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-order": "3.0.0",
    "terser-webpack-plugin": "1.2.3",
    "typescript": "3.4.5",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.2.1"
  },
  "dependencies": {
    "@types/jquery": "3.3.29",
    "jquery": "3.4.1",
    "lint-staged": "8.1.5",
    "mdn-polyfills": "5.16.0",
    "slick-carousel": "1.8.1",
    "velocity-animate": "1.5.2",
    "whatwg-fetch": "^3.0.0"
  },
  "browserslist": [
    ">0.25%",
    "not dead",
    "not ie <= 10"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "debug": true
        }
      ],
      "@babel/preset-typescript",
      "react-app"
    ]
  }
}
