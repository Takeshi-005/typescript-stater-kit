{
    "compilerOptions": {
      "outDir": "./dist/assets/js",
      // "noImplicitAny": true,
      "lib": [
            "dom",
            "es7",
            "es2015.promise",
            "es5"
      ],
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "sourceMap": true
    },
    "include": [
      "src/js/*",
      "src/js/**/*",
    ],
    "exclude": [
      "node_modules",
      "**/*.spec.ts"
    ]
  }
